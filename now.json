{
  "version": 2,
  "name": "blog",
  "routes": [
    { "src": "/.*", "dest": "index.js" }
  ],
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node-server",
      "config": { "maxLambdaSize": "20mb" }
    }
  ],
  "alias": [
    "https://blog.zce.now.sh"
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": [
    "tpe"
  ]
}
