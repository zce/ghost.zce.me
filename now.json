{
  "version": 2,
  "name": "blog",
  "builds": [
    { "src": "index.js", "use": "@now/node-server", "config": { "maxLambdaSize": "30mb" } }
  ],
  "routes": [
    { "src": "/.*", "dest": "index.js" }
  ],
  "alias": [
    "https://blog.zce.now.sh"
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": [
    "hnd"
  ]
}
